#!/bin/bash
source bash_common.sh
if [ $# -ne 1 ]; then
  echo "Print list of objects deleted from uniColl..Hybrid"
  echo "Update: uniColl..Hybrid"
  echo "#1: output file"
  exit 1
fi

sqsh-ms  -S ""  -D uniColl  -L exit_failcount=1 << EOF | sed 's/|$//1' > $1
  EXEC Genome_unhybrid 2759;  -- PAR
  go -m bcp
EOF
